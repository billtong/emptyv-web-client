{
  "name": "empty-video-web",
  "version": "1.0.0",
  "description": "a video sharing website front with DanMu build in React.js",
  "main": "index.js",
  "scripts": {
    "start": "node bin/start.js",
    "build": "webpack --config bin/config/webpack.prod.config.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/billtong/empty-video-web.git"
  },
  "keywords": [
    "Empty",
    "Video"
  ],
  "author": "EmptyVideo Ltd.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/billtong/empty-video-web/issues"
  },
  "homepage": "https://github.com/billtong/empty-video-web#readme",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "last 2 versions",
              "iOS >= 7",
              "Android >= 5"
            ]
          },
          "useBuiltIns": "usage"
        }
      ],
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "semi": "error"
    },
    "env": {
      "browser": true,
      "node": true
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "^0.15.3",
    "express": "^4.14.0",
    "npm": "^6.9.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.1",
    "react-router": "^3.0.5",
    "react-icons": "^3.4.0",
    "react-spinners": "^0.5.3",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "react-dev-utils": "^8.0.0",
    "react-hot-loader": "^4.8.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "node-sass": "^4.5.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
